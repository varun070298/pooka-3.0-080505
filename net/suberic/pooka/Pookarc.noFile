#This is the resource file for Pooka.  Note that all of the labels and key
#bindings for the menus, etc., are found in locale/Pooka_<Locale>.properties

Pooka.debug=false
Pooka.sessionDebug=false

#Pooka.useLocalFiles=true
Pooka.useLocalFiles=false
Pooka.httpConfig=true

Pooka.cacheDirectory=~/Mail/pooka
Pooka.autoCache=true
Pooka.autoPreview=true
Pooka.autoPreview.propertyType=Boolean
Pooka.autoSelectFirstUnread=true
Pooka.autoSelectFirstUnread.propertyType=Boolean

Message.sendImmediately=true

Pooka.hsize=800
Pooka.vsize=600

Pooka.guiType=Desktop
Pooka.guiType.propertyType=List
Pooka.guiType.allowedValues=Desktop:Preview

Pooka.useTrashFolder=true

Pooka.useAddressCompletion=false

Pooka.folderPanel.hsize=180
Pooka.folderPanel.vsize=570
