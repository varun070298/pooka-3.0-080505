Pooka.messagePanel.vsize=570

Pooka.previewPanel.folderSize=200

Pooka.attachmentPanel.vsize=100

Pooka.searchFolderDisplay.hsize=500
Pooka.searchFolderDisplay.vsize=100

Pooka.addressBookEditor.hsize=300
Pooka.addressBookEditor.vsize=100

Pooka.replyPrefix=>
Pooka.forwardPrefix=>
Pooka.forwardStyle=prefixed
Pooka.forwardStyle.propertyType=List
Pooka.forwardStyle.allowedValues=prefixed:not prefixed

Pooka.lineWrap=true
Pooka.lineWrap.propertyType=Boolean
Pooka.lineLength=76
Pooka.displayTextAttachments=true
Pooka.displayTextAttachments.propertyType=Boolean
