Pooka.openSavedFoldersOnStartup=true
Pooka.openSavedFoldersOnStartup.propertyType=Boolean

Pooka.saveOpenFoldersOnExit=true
Pooka.saveOpenFoldersOnExit.propertyType=Boolean

Pooka.saveFolderWindowLocationsOnExit=true
Pooka.saveFolderWindowLocationsOnExit.propertyType=Boolean

Pooka.newMessage.autoAddSignature=true
Pooka.newMessage.autoAddSignature.propertyType=Boolean
Pooka.newMessage.signatureBeforeCopiedMessage=false
Pooka.newMessage.signatureBeforeCopiedMessage.propertyType=Boolean
Pooka.defaultSigGenerator=net.suberic.pooka.FileSignatureGenerator

Pooka.autoExpunge=true
Pooka.autoExpunge.propertyType=Boolean

Pooka.checkUnsentMessages=true
Pooka.checkUnsentMessages.propertyType=Boolean

Pooka.timeout=30000
Pooka.connectionTimeout=30000

Pooka.fetchBatchSize=50

Pooka.xmailer=Pooka
Pooka.userProfileProperty=X-Pooka-UserProfile

