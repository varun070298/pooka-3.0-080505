Pooka.displayHtml=true
Pooka.displayHtml.propertyType=Boolean
Pooka.displayHtmlAsDefault=false
Pooka.displayHtmlAsDefault.propertyType=Boolean

Pooka.openFoldersOnConnect=true
Pooka.openFoldersOnConnect.propertyType=Boolean
